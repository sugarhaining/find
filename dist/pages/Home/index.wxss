@charset "UTF-8";
@font-face {
  font-family: 'webfont';
  font-display: swap;
  src: url("//at.alicdn.com/t/webfont_yt9f4zs4gpp.eot");
  /* IE9*/
  src: url("//at.alicdn.com/t/webfont_yt9f4zs4gpp.eot?#iefix") format("embedded-opentype"), url("//at.alicdn.com/t/webfont_yt9f4zs4gpp.woff2") format("woff2"), url("//at.alicdn.com/t/webfont_yt9f4zs4gpp.woff") format("woff"), url("//at.alicdn.com/t/webfont_yt9f4zs4gpp.ttf") format("truetype"), url("//at.alicdn.com/t/webfont_yt9f4zs4gpp.svg#站酷小薇体") format("svg");
  /* iOS 4.1- */
}
.container {
  display: flex;
  align-items: center;
  flex-direction: column;
  background: url("data:image/jpeg;base64,") no-repeat;
  background-size: cover;
  background-position: 0 -220rpx;
}
.panner {
  margin-top: 40vh;
  width: 100vw;
  min-height: 60vh;
  padding-top: 120rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
  padding-bottom: 30rpx;
  box-sizing: border-box;
  background-color: #fff;
  border-radius: 40rpx 40rpx 0 0;
  position: relative;
  display: flex;
  align-items: center;
  flex-direction: column;
}
.avatar {
  width: 150rpx;
  height: 150rpx;
  position: absolute;
  left: 80rpx;
  top: -50rpx;
  background-color: #fff;
  border-radius: 50%;
  box-shadow: 0 0 60rpx 4rpx #bbb;
  box-sizing: border-box;
}
.login {
  align-self: flex-start;
  margin-left: 80rpx;
  font-size: 40rpx;
  font-weight: 400;
}
.comment {
  align-self: flex-start;
  margin-top: 30rpx;
  margin-left: 30rpx;
  font-size: 26rpx;
  position: relative;
}
.comment::after {
  content: '';
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 10rpx;
  background-color: #FFD531;
  border-radius: 10rpx;
  opacity: .6;
}
.menus {
  width: 100%;
  margin-top: 10rpx;
  display: flex;
  align-items: center;
  justify-content: space-around;
  flex-wrap: wrap;
  font-size: 34rpx;
}
.swiper {
  width: 100vw;
}
.swiper .item {
  width: 80%;
  padding: 40rpx 80rpx;
  box-sizing: border-box;
  position: relative;
}
.swiper .item .inner {
  width: 80%;
  height: 80%;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  box-shadow: 2rpx 2rpx 30rpx 1rpx #eee;
  border-radius: 15rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate3d(-50%, -50%, 0);
          transform: translate3d(-50%, -50%, 0);
  display: flex;
  align-items: center;
  flex-direction: column;
  align-items: flex-start;
}
.swiper .item .inner .mood {
  font-size: 40rpx;
}
.swiper .item .inner .commend {
  margin-top: 10rpx;
  font-size: 26rpx;
}
.menu {
  width: 46%;
  padding: 10rpx 10rpx;
  margin-top: 20rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-around;
  height: 110rpx;
  border-radius: 20rpx;
  background-color: #fff;
  box-shadow: 3rpx 6rpx 30rpx 10rpx #eee;
}
.menu image {
  width: 50rpx;
  height: 50rpx;
}